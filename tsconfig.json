{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@env/*": [
        "environments/*"
      ],
      "@sita/matcore": [
        "../dist/sita/matcore"
      ],
      "@sita/matcore/*": [
        "../dist/sita/matcore/*"
      ],
      "@sita/login": [
        "dist/sita/login"
      ],
      "@sita/login/*": [
        "dist/sita/login/*"
      ]
    }
  }
}